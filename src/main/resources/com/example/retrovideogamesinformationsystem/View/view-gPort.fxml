<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.retrovideogamesinformationsystem.Controllers.GPController">

    <Label layoutX="192.0" layoutY="35.0" text="VIEWING GAME PORTS">
        <font>
            <Font size="21.0" />
        </font></Label>

    <Button layoutX="74.0" layoutY="90.0" onAction="#displayGamePort" prefHeight="25.0" prefWidth="146.0" text="Display" />
    <Button layoutX="231.0" layoutY="90.0" onAction="#ascendingSortDisplay" prefHeight="25.0" prefWidth="146.0" text="Year Ascending" />
    <Button layoutX="231.0" layoutY="130.0" onAction="#descendingSortDisplay" prefHeight="25.0" prefWidth="146.0" text="Year Descending" />
    <Button layoutX="74.0" layoutY="130.0" onAction="#switchToViewSystem" prefHeight="25.0" prefWidth="146.0" text="Back" />
    <Button layoutX="396.0" layoutY="130.0" prefHeight="25.0" prefWidth="138.0" text="Search For Name" onAction="#searching" />
    <TextField fx:id="searchTF" layoutX="396.0" layoutY="90.0" prefHeight="25.0" prefWidth="138.0" promptText="Search Name of Game Machine" />

    <ScrollPane layoutX="74.0" layoutY="170.0" prefHeight="185.0" prefWidth="460.0">

        <content>
            <Label fx:id="display" />
        </content>
    </ScrollPane>
</AnchorPane>
